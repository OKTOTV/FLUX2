{
    "minimum-stability": "dev",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.0.8",
        "bprs/asset-bundle": "^3.0",
        "bprs/user-bundle": "dev-master",
        "doctrine/orm": "^2.6",
        "friendsofsymfony/elastica-bundle": "^5.0",
        "jms/serializer-bundle": "^2.3",
        "knplabs/knp-markdown-bundle": "^1.5",
        "league/flysystem-aws-s3-v3": "^1.0@dev",
        "okto/media-bundle": "dev-master",
        "oktolab/media-bundle": "dev-master",
        "oneup/flysystem-bundle": "^3.0",
        "sensio/framework-extra-bundle": "^5.1",
        "sofort/sofortlib-php": "^3.3",
        "symfony/console": "^3.4",
        "symfony/expression-language": "^3.4",
        "symfony/flex": "^1.0",
        "symfony/form": "^3.4",
        "symfony/framework-bundle": "^3.4",
        "symfony/lts": "^3",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/profiler-pack": "dev-master",
        "symfony/security-bundle": "^3.4",
        "symfony/security-csrf": "^3.4",
        "symfony/serializer": "^3.4",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/twig-bundle": "^3.4",
        "symfony/validator": "^3.4",
        "symfony/yaml": "^3.4"
    },
    "require-dev": {
        "symfony/dotenv": "^3.4",
        "symfony/web-server-bundle": "^3.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C3APM1GZSEWEBJNXRHESBM49",
            "allow-contrib": false
        }
    }
}
